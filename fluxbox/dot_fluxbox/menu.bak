[begin] (Fluxbox) {}
    [exec] (quadkonsole) {quadkonsole --rows 2 --columns 2}
    [exec] (mrxvt) {mrxvt} </usr/share/pixmaps/mrxvt.xpm>
    [exec] (firefox) {firefox} </usr/share/pixmaps/firefox-icon.png>
    [exec] (ROX) {rox} </usr/share/pixmaps/ROX-Filer.png>
    [exec] (dolphin) {dolphin} </usr/share/icons/hicolor/32x32/apps/dolphin.png>
    [exec] (Claws) {claws-mail} </usr/share/pixmaps/claws-mail.png>
    [exec] (xchat) {xchat} </usr/share/pixmaps/xchat.png>
    [exec] (azureus) {azureus} </usr/share/pixmaps/azureus.png>
    [exec] (ktorrent) {ktorrent} </usr/share/apps/ktorrent/www/default/icon.png>
    [exec] (gmpc) {gmpc} </usr/share/pixmaps/gmpc.png>
    [exec] (opera) {opera} </usr/share/pixmaps/opera.xpm>
    [exec] (incollector) {incollector} </usr/share/pixmaps/incollector.png>
    [exec] (gajim) {gajim} </usr/share/pixmaps/gajim.png>
    [exec] (pidgin) {pidgin} </usr/share/pixmaps/pidgin/emotes/default/hypnotized.png>
    [exec] (virtual box) {VirtualBox} </usr/share/pixmaps/virtualbox.png>
    [separator]
    [exec] (Run) {fbrun}
    [separator]
    [submenu] (Office)
        [exec] (writer) {/usr/bin/oowriter} </usr/share/pixmaps/ooo-writer.png>
        [exec] (calc) {/usr/bin/oocalc} </usr/share/pixmaps/ooo-calc.png>
        [exec] (impress) {/usr/bin/ooimpress} </usr/share/pixmaps/ooo-impress.png>
    [end]
    [submenu] (system) {}
        [exec] (wine) {/opt/cxoffice/bin/cxsetup}
        [exec] (VMware) {vmware} </usr/share/pixmaps/vmware-workstation.png>
        [exec] (G-Top) {gnome-system-monitor}
        [exec] (Komodo) {komodo}
        [exec] (xcdroast) {xcdroast}
    [end]
    [submenu] (Terminals) {}
        [exec] (xterm) {xterm}
        [exec] (gnome-terminal) {gnome-terminal}
        [exec] (konsole) {konsole}
        [exec] (aterm) {aterm}
        [exec] (rxvt) {rxvt -bg black -fg green}
    [end]
    [submenu] (Net) {}
        [exec] (gftp) {gftp} </usr/share/pixmaps/gftp.png>
        [exec] (filezilla) {filezilla} </usr/share/pixmaps/filezilla.png>
        [exec] (pan) {pan} </usr/share/pixmaps/pan.png>
    [end]
    [submenu] (Browsers) {}
        [exec] (mozilla) {mozilla}
        [exec] (konqueror) {kfmclient openProfile webbrowsing}
        [exec] (links) {aterm -e links fluxbox.org}
        [exec] (lynx) {aterm -e lynx fluxbox.org}
    [end]
    [submenu] (Editors) {}
        [exec] (gvim) {gvim}
        [exec] (gedit) {gedit}
        [exec] (xedit) {xedit}
        [exec] (kword) {kword}
        [exec] (kwrite) {kwrite}
        [exec] (kate) {kate}
    [end]
    [submenu] (Multimedia) {}
        [submenu] (Graphics) {}
            [exec] (gimp) {gimp}
            [exec] (xv) {xv}
            [exec] (xpaint) {xpaint}
            [exec] (kpaint) {kpaint}
            [exec] (kiconedit) {kiconedit}
            [exec] (xscreensaver-demo) {xscreensaver-demo}
        [end]
        [submenu] (Music) {}
            [exec] (xmms) {xmms}
            [exec] (kmix) {kmix}
        [end]
        [submenu] (Video) {}
            [exec] (xine) {xine}
            [exec] (aviplay) {aviplay}
            [exec] (gmplayer) {gmplayer}
            [exec] (dvdrip) {nohup dvdrip}
        [end]
        [submenu] (X-utils) {}
            [exec] (xfontsel) {xfontsel}
            [exec] (xman) {xman}
            [exec] (xload) {xload}
            [exec] (editres) {editres}
            [exec] (viewres) {viewres}
            [exec] (xclock) {xclock}
        [end]
    [end]
    [submenu] (Games) {}
        [exec] (gnibbles) {gnibbles}
        [exec] (gnobots2) {gnobots2}
        [exec] (gataxx) {gataxx}
        [exec] (glines) {glines}
        [exec] (gnect) {gnect}
        [exec] (mahjongg) {mahjongg}
        [exec] (gnomine) {gnomine}
        [exec] (gnome-stones) {gnome-stones}
        [exec] (gnometris) {gnometris}
        [exec] (gnotravex) {gnotravex}
        [exec] (gnotski) {gnotski}
        [exec] (iagno) {iagno}
        [exec] (same-gnome) {same-gnome}
        [exec] (xboard) {xboard}
        [exec] (gtali) {gtali}
        [exec] (xsnow) {xsnow}
        [exec] (xeyes) {xeyes}
    [end]
    [submenu] (Fluxbox-menu) {}
        [config] (Configure) {}
        [submenu] (Styles) {Choose a style...}
            [stylesdir] (/usr/share/fluxbox/styles) {}
            [stylesdir] (~/.fluxbox/styles) {}
        [end]
        [workspaces] (Workspace List) {}
        [submenu] (Tools) {}
            [exec] (FluxConf) {fluxconf}
            [exec] (FluxKeys) {fluxkeys}
            [exec] (FluxMenu) {fluxmenu}
            [exec] (Windowname) {xprop|grep WM_CLASS|cut -d \" -f 2|xmessage -file - -center}
            [exec] (Run) {fbrun}
        [end]
        [Reconfigure] (Reload config) {}
        [restart] (Restart) {}
        [exit] (Exit) {}
    [end]
[end]
